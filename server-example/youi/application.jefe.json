{
  "proxies": [
    {
      "enabled": true,
      "inbound": {
        "port": 8080,
        "domains": [
          "youi.io",
          "www.youi.io",
          "darkfrog.youi.io"
        ]
      },
      "outbound": {
        "url": "http://localhost:8889"
      }
    }
  ],
  "applications": [
    {
      "type": "dependency",
      "enabled": true,
      "group": "io.youi",
      "artifact": "youi-example",
      "version": "$version",
      "mainClass": "io.youi.example.ServerExampleApplication",
      "scalaVersion": "2.12"
    }
  ]
}